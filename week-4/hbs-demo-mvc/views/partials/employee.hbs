 {{#if visible}}
    <tr>
        <td>{{name}}</td>
        <td>{{age}}</td>
        <td>{{occupation}}</td>
        <td>{{company}}</td>
        <td><img src="/images/employeeImages/{{imageUrl}}" height="50" alt="Picture of {{name}}"/></td>
    </tr>
{{else}}
    <tr>
        <td colspan="5">User {{name}} has hidden their information.</td>
    </tr>
{{/if}}